{{> header}}


<div>

<form action="/addOrganism" method="POST">
    <div class="form-inline">
        <label for="Organism">Organism: </label>
        <input type="text" class="form-control" id="Organism" name="Organism">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

<table id="Interactome" style="border:  1px solid black">
    <thead>
      <th>Organism</th>
    </thead>
    <tbody id="Interactome_body" style="border:  1px solid black">
      {{#data.inters}}
      <tr>
          <td id="id"style="display:none">{{id}}</td>
          <td id="Organism" name="Organism">{{Organism}}</td>
          <td id="delete"> <input type="button" value="delete" onclick="deleteInteraction({{id}})" /></td>
          <td id="edit"> <input type="button" value="edit" onClick="modifyRow(this)"/></td>
      </tr>
      {{/data.inters}}
    </tbody>
</table>

</div>