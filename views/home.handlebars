<form method="POST" action="/addOrtho" id="orthoData"> 
  <div class="form-inline">
    <label for="ProteinID">ProteinId A: </label>
    <input type="text" class="form-control" id="Input_ProteinIDA" name="Input_ProteinIDA">
  </div>
  <div class="form-inline">
    <label for="ProteinID">NCBI_GeneID: </label>
    <input type="text" class="form-control" id="In_GeneID" name="In_GeneID">
  </div>
  <div class="form-inline">
    <label for="ProteinID">Annotation Protein A: </label>
    <input type="text" class="form-control" id="Ann_ProteinIDA" name="Ann_ProteinIDA">
  </div>
