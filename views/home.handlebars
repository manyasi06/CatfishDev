{{> header}}
<div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
  <ol class="carousel-indicators">
    <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
  </ol>
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img class="d-block w-100" src="/images/corn.jpg" alt="First slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="/images/corn2.jpg" alt="Second slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="/images/fungus.jpg" alt="Third slide">
    </div>
  </div>
  <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>

<form>
  <div class="form-inline">
    <label for="ProteinID">ProteinId:</label>
    <input type="text" class="form-control" id="Input_ProteinID">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>


<br>
<table id="Interactome" style="border:  1px solid black">
    <thead>
      <th>ProteinID_A</th>
      <th>ProteinID_B</th>
      <th>Annotation_A</th>
      <th>Annotation_B</th>
      <th>Organism</th>
    </thead>
    <tbody id="Interactome_body" style="border:  1px solid black">
      {{!-- {{#data.Interactome}} --}}
      <tr>
          <td id="P_ida">{{ProteinID_A}}</td>
          <td id="P_idb">{{ProteinID_B}}</td>
          <td id="PA_anno">{{PA_annotation}}</td>
          <td id="PB_anno">{{PB_annotation}}</td>
          <td id="Organism">{{Organism}}</td>
          <td id="delete"> <input type="button" value="delete" onclick="del_row(this)" /></td>
          <td id="edit"> <input type="button" value="edit" onClick="modifyRow(this)"/></td>
      </tr>
      {{!-- {{/data.Interactome}} --}}
    </tbody>
</table>


