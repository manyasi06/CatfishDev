  <select name="Organism">
    {{#each someData}}
      <option value="{{this}}">{{this}}</option>
    {{/each}}
  </select>
  <div class="form-inline">
    <label for="ProteinID">ProteinId B: </label>
    <input type="text" class="form-control" id="Input_ProteinIDB" name="Input_ProteinIDB">
  </div>
  <div class="form-inline">
    <label for="ProteinID">Experimental Condition: </label>
    <input type="text" class="form-control" id="Exp_Val" name="Exp_Val">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>


<br>
<form method="POST" action="/addGeneExp"> 
  <div class="form-inline">
    <label for="ProteinID">ProteinId:</label>
    <input type="text" class="form-control" id="GID" name="GID">
  </div>
  <div class="form-inline">
    <label for="Protein">Sample Name</label>
    <input type="text" class="form-control" id="SID" name="SID">
  </div>
  <div class="form-inline">
    <label for="Protein">Add Gene Expression</label>
    <input type="number" step="any" class="form-control" id="ExpreVal" name="ExpreVal">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>


<br>
<table id="Interactome" style="border:  1px solid black">
    <thead>
      <th>ProteinID_A</th>
      <th>ProteinID_B</th>
      <th>Annotation_A</th>
      <th>Organism</th>
    </thead>
    <tbody id="Interactome_body" style="border:  1px solid black">
      {{#data.inters}}
      {{!-- {{#each data.Interactome}} --}}
      <tr>
          <td id="id" style="display:none;">{{id}} </td>
          <td id="P_ida">{{ProteinID_A}}</td>
          <td id="P_idb">{{ProteinID_B}}</td>
          <td id="PA_anno">{{PA_annotation}}</td>
          <td id="Organism">{{Organism}}</td>
          <td id="delete"> <input type="button" value="delete" onclick="deleteInteraction(this)" /></td>
          <td id="edit"> <input type="button" value="edit" onClick="modifyRow(this)"/></td>
      </tr>
      {{/data.inters}}
    </tbody>
</table>

